174     {\cf18 size_t} tamanhoNome = valor.length();\par
175     {\cf18 int} quantosNomes = 1;\par
176     {\cf18 int} posEspaco;\par
177 \par
178     {\cf20 //Descobre quantos termos tem o nome e salva posicao do espaco ' '.}\par
179     {\cf20 //Tambem verifica se todos os caracteres sao validos.}\par
180     {\cf19 for} ({\cf18 size_t} i=0; i<tamanhoNome; i++)\{\par
181         {\cf19 if}(valor[i] == {\cf23 ' '})\{\par
182             quantosNomes++;\par
183             posEspaco = i;\par
184         \}\par
185         {\cf19 if}(isalpha(valor[i]) == 0 && valor[i] != {\cf23 ' '})\{\par
186             {\cf19 throw} invalid_argument({\cf22 "Nome com caractere invalido."});\par
187         \}\par
188     \}\par
189 \par
190     {\cf20 //Verifica se excedeu o numero maximo de nomes.}\par
191     {\cf19 if}(quantosNomes > 2) \{\par
192         {\cf19 throw} invalid_argument({\cf22 "Numero maximo de nomes (2) excedido."});\par
193     \}\par
194 \par
195     {\cf20 //Salva o tamanho dos termos em caso de nome composto.}\par
196     {\cf18 size_t} tamNome1, tamNome2;\par
197     {\cf18 string} nome1, nome2;\par
198     {\cf19 if}(quantosNomes == 2)\{\par
199         nome1 = valor.substr(0, posEspaco);\par
200         nome2 = valor.substr(posEspaco + 1);\par
201 \par
202         tamNome1 = nome1.length();\par
203         tamNome2 = nome2.length();\par
204     \}\par
205 \par
206     {\cf20 //Verifica se os termos tem a quantidade correta de caracteres}\par
207     {\cf19 if}((quantosNomes == 1 && (tamanhoNome > 10 || tamanhoNome < 3))\par
208         || (quantosNomes == 2 && (tamNome1 > 10 || tamNome1 < 3 || tamNome2 > 10 || tamNome2 < 3)))\{\par
209         {\cf19 throw} invalid_argument({\cf22 "Numero maximo de caracteres por nome excedido."});\par
210     \}\par
211 \par
212     {\cf20 //Verifica se algum dos termos comeca com letra maiuscula}\par
213     {\cf19 if}((!isupper(valor[0]) && quantosNomes == 1)\par
214         ||(quantosNomes == 2 && (!isupper(nome1[0]) || !isupper(nome2[0])))) \{\par
